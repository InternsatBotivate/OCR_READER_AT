{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/worker.js",
      "dest": "/worker.js"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/ocr",
      "dest": "main.py"
    }
  ]
}
